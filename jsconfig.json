{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/public/*": [
        "public/*"
      ],
      "@/app/*": [
        "src/app/*"
      ],
      "@/api/*": [
        "src/app/api*"
      ],
      "@/providers/*": [
        "src/providers/*"
      ],
      "@/models/*": [
        "src/models/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/constant/*": [
        "src/constant/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/modules/*": [
        "src/components/modules/*"
      ],
      "@/layout/*": [
        "src/components/layout/*"
      ],
      "@/templates/*": [
        "src/components/utils/*"
      ],
    }
  }
}